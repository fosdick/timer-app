{
  "expo": {
    "name": "TimerAppYoga",
    "slug": "timer-app-yoga",
    "version": "1.0.4",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#080B0c"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.example.timer-app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#080B0c"
      },
      "package": "com.stuffingcat.timerapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-9298793039154739~3188608061",
          "iosAppId": "ca-app-pub-9298793039154739~3188608061"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "eb3f4eef-fafa-4d8e-8942-3604f7ea39a9"
      }
    },
    "owner": "stuffingcat",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/eb3f4eef-fafa-4d8e-8942-3604f7ea39a9"
    }
  }
}
