{
  "expo": {
    "name": "TimerAppYoga",
    "slug": "timer-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#080B0c"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.example.timer-app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#080B0c"
      },
      "package": "com.stuffingcat.timerapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      ["expo-router"],
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "405b9d5d-821f-4302-b1fc-f4c071739dfc"
      }
    },
    "owner": "stuffingcat",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/405b9d5d-821f-4302-b1fc-f4c071739dfc"
    }
  }
}
